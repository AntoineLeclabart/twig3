{% extends 'layout.html.twig' %}
{% block title %} Products {% endblock %}
{% block content %}
    {% for product in products %}
        {% if loop.index < 4 %}
            {{product}}
        {% endif %} 
    {% endfor %}
    <br/>
    {% for product in products %}
        {% if loop.index > 3 %}
            {{product}}
        {% endif %} 
    {% endfor %}
    {{dump(products)}}
{% endblock %}
